  "keywords": [
    "preact",
    "preact-compat",
    "react",
    "redux",
    "sass",
    "yarn"
  ],
  "main": "index.js",
  "bundlesize": [
    {
      "path": "build/bundlesize.gz",
      "maxSize": "100 kB"
    }
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "prestaging": "rm -rf build mkdirp build && yarn build && now switch soundplace --token $NOW_TOKEN",
    "predeploy": "rm -rf build mkdirp build && yarn build",
    "deploy": "gh-pages -d build",
    "staging": "URL=$(now ./build -n staging-soundplace --token $NOW_TOKEN --static --public -f) && now alias $URL staging-soundplace -token $NOW_TOKEN -f",
    "dev": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server",
    "prebuild": "rm -rf build mkdirp build",
    "prestart": "yarn build",
    "test": "karma start test/karma.conf.js --single-run",
    "lint": "eslint src/**/*.js test/**/*.js",
    "test:watch": "karma start test/karma.conf.js --auto-watch",
    "prebundlesize": "gzip -c build/index.html > build/bundlesize.gz",
    "bundlesize": "bundlesize"
  }
}
